    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>getPubKey</servlet-name>
    <servlet-class>servlet.getPubKey</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>sendMess</servlet-name>
    <servlet-class>servlet.sendMess</servlet-class>
  </servlet>
  <servlet>
